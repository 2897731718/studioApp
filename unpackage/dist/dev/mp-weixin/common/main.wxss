	color:#70a1ff;
	/* width:40px;
 */
	/* height:40px;
 */
	/* line-height:100px;
 */
	/* margin:0 16px;
 */
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	        justify-content: center;
	-webkit-transition: .2s linear;
	transition: .2s linear;
	/* border: 2px solid #0FB9B1;
 */
	position: relative;
}
.btn-rotate::before {
	content:"";
	position: absolute;
	box-sizing: border-box;
	width:100%;
	height:100%;
	left:0;
	top:0;
	-webkit-transition: .2s linear;
	transition: .2s linear;
}
.btn-rotate::after{
	content:"";
	position: absolute;
	box-sizing: border-box;
	width:100%;
	height:100%;
	left:0;
	top:0;
	-webkit-transition: .2s linear;
	transition: .2s linear;
}
.btn-rotate:active{
	/* 改变大小 原始宽度的 0.8 倍
 */
    -webkit-transform: scale(.8);
            transform: scale(.8);
}
.btn-rotate:active::before{
    border-left:2px solid;
    border-right:2px solid;
	/* 延 x 轴倾斜给定角度
